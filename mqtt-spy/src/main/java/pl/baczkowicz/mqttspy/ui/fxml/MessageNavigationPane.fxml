<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="30.0" prefWidth="518.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.baczkowicz.mqttspy.ui.MessageNavigationController">
	<children>      
      <HBox fx:id="messageIndexBox" layoutX="8.0" layoutY="3.0" prefHeight="22.0" prefWidth="271.0">
         <children>
      		<Label fx:id="messageLabel" prefHeight="27.0" text="No messages" />
         </children>
      </HBox>
      <CheckBox fx:id="showLatestBox" focusTraversable="false" layoutX="279.0" layoutY="6.0"  mnemonicParsing="false" selected="true" text="Show latest on new" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="6.0">
         <font>
            <Font size="11.0" />
         </font>
      </CheckBox>
      
      <!-- text="&gt;"  -->
      <Button fx:id="lessRecentButton" focusTraversable="false" layoutX="449.0" layoutY="2.0" prefHeight="26" mnemonicParsing="false" onAction="#showLessRecent" AnchorPane.rightAnchor="35.0" AnchorPane.topAnchor="2.0">
      <graphic>
      		<ImageView fx:id="settingsIcon" fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
		         <image>
		            <Image url="/images/go-next.png" />
		         </image>
		      </ImageView>
      	</graphic>
         <font>
            <Font size="11.0" />
         </font>
      </Button>
         <!-- text="&lt;"  -->
      <Button fx:id="moreRecentButton" focusTraversable="false" layoutX="414.0" layoutY="2.0" prefHeight="26" mnemonicParsing="false" onAction="#showMoreRecent" AnchorPane.rightAnchor="70.0" AnchorPane.topAnchor="2.0">
                <graphic>
      		<ImageView fx:id="settingsIcon" fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
		         <image>
		            <Image url="/images/go-previous.png" />
		         </image>
		      </ImageView>
      	</graphic>
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <!-- text="&gt;|"  -->   
      <Button fx:id="showLastButton" focusTraversable="false" layoutX="484.0" layoutY="2.0" prefHeight="26" mnemonicParsing="false" onAction="#showLast" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="2.0">
      	<graphic>
      		<ImageView fx:id="settingsIcon" fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
		         <image>
		            <Image url="/images/go-last.png" />
		         </image>
		      </ImageView>
      	</graphic>
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <!-- text="|&lt;"  -->
      <Button fx:id="showFirstButton" focusTraversable="false" layoutX="379.0" layoutY="2.0" prefHeight="26" mnemonicParsing="false" onAction="#showFirst" AnchorPane.rightAnchor="105.0" AnchorPane.topAnchor="2.0">
          <graphic>
      		<ImageView fx:id="settingsIcon" fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
		         <image>
		            <Image url="/images/go-first.png" />
		         </image>
		      </ImageView>
      	</graphic>
         <font>
            <Font size="11.0" />
         </font></Button>      
	</children>
</AnchorPane>
